{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
		"indent": [
			true,
			"tabs",
			4
		],
		"object-literal-sort-keys": [
			false
		],
		"max-line-length": [
			false
		],
		"no-console": [
			false
		],
		"no-string-literal": false,
		"one-line": [
			false
		],
		"no-trailing-whitespace": false,
		"align": [
			false
		],
		"forin": false
	},
	"rulesDirectory": []
}